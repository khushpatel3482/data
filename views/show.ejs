<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Users</title>
    <style>
        table,tr,td,th{
            border: 1px solid black;
        }
        button{
            border-radius: 15px;
            background-color: red;
            color: white;
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Username</th>
            <th>Change</th>
        </tr>
        <%for(user of users){ %>
            <tr>
                <td><%=user.id%></td>
                <td><%=user.email%></td>
                <td><%=user.username%></td>
                <td>
                <form method ="get" action="/user/<%=user.id%>/edit">
                    <button>Edit username</button>
                </form>
                </td>
            </tr>
        <% }%>
    </table>
</body>
</html>

